//find the first position 1 based that yields the basement (floor -1)
let mutable floor = 0
let mutable index = 0
exception BasementFound

try
    for char in directions do
        floor <- floor + (if char = '(' then 1 else -1)
        index <- index + 1
        if (floor = -1) then
            raise BasementFound
